{
  "_id": "200000000000000000000011",
  "type": "layout",
  "contentStyle": {
    "display": "flex",
    "flexFlow": "row wrap",
    "height": 0,
    "flexGrow": 1
  },
  "layoutStyle": {
    "width": "100%",
    "height": "100%",
    "display": "flex",
    "flexFlow": "row wrap",
    "overflow-y": "auto"
  },
  "actions": [
    {
      "type": "button",
      "buttonType": "mat-icon-button",
      "label": "Salva",
      "click": "this.event.send({name: 'save'})",
      "icon": "save"
    },
    {
      "type": "button",
      "buttonType": "mat-icon-button",
      "label": "Refresh",
      "click": "this.event.send({name: 'refresh'})",
      "icon": "refresh"
    }
  ],
  "screens": [
    {
      "type": "tabs",
      "init": "obj.set(this.data, '_params_.initial', { company_id: this.data._id });\r\nobj.set(this.data, '_params_.theme', { company_id: this.data._id } );\r\nobj.set(this.data, '_params_.permission', { filter: {company_id: this.data._id} } );\r\nobj.set(this.data, '_params_.users', { filter: {company_id: this.data._id} } );",
      "eventHandler": "if (event.name == 'save') {\r\n    this.event.send({ name: 'splash-show' });\r\n    this.rest.request(`/api/admin/core.company`, this.data, 'post')\r\n        .subscribe(r => {\r\n            this.data._id = r._id;\r\n            this.event.send({ name: 'splash-hide' });\r\n            this.snackBar.open('Salvato', null, { duration: 2000 });\r\n        })\r\n}",
      "layoutStyle": {
        "width": "100%",
        "height": "100%"
      },
      "style": {
        "width": "100%",
        "height": "100%"
      },
      "color": "accent",
      "backgroundColor": "primary",
      "screens": [
        {
          "label": "SITE",
          "type": "ui-element-id",
          "uiElementId": "200000000000000000000012"
        },
        {
          "label": "FEATURE",
          "type": "ui-element-id",
          "uiElementId": "200000000000000000000013"
        },
        {
          "label": "INITIAL SCREEN",
          "key": "_params_.initial",
          "type": "ui-element-id",
          "uiElementId": "200000000000000000000014"
        },
        {
          "label": "THEME",
          "key": "_params_.theme",
          "type": "ui-element-id",
          "uiElementId": "200000000000000000000015"
        },
        {
          "label": "PERMISSION",
          "key": "_params_.permission",
          "type": "ui-element-id",
          "uiElementId": "200000000000000000000120"
        },
        {
          "label": "USERS",
          "key": "_params_.users",
          "type": "ui-element-id",
          "uiElementId": "200000000000000000000110"
        }
      ]
    }
  ]
}