{
  "_id": ObjectId("000000000000000000000000"),
  "user": {
    "class": {
      "mat-elevation-z2": true
    },
    "style": {
      "color": "white",
      "margin": 0,
      "padding": 0,
      "background": "crimson"
    },
    "header": {
      "screen": [
        {
          "type": "button",
          "buttonType": "mat-icon-button",
          "icon": "account_circle",
          "style": {
            "position": "absolute",
            "left": 0
          }
        },
        {
          "type": "typography",
          "default": "`${this.user.name()}`",
          "style": {
            "marginLeft": "24px"
          }
        }
      ]
    },
    "menu": {
      "screen": [
        {
          "type": "button",
          "buttonType": "mat-menu-item",
          "label": "Profile",
          "click": "this.nav.navigateByUrl(`/profile`)"
        },
        {
          "type": "button",
          "buttonType": "mat-menu-item",
          "label": "Logout",
          "click": "this.event.send('logout')"
        }
      ]
    }
  },
  "navigation": {
    "style": {
      "width": "256px"
    },
    "header": {
      "screen": [
        {
          "type": "typography",
          "text": "Admin",
          "style": {
            "fontSize": "20px",
            "textAlign": "center",
            "padding": "24px 8px 12px"
          }
        }
      ]
    }
  },
  "toolbar": {
    "layoutStyle": {
      "height": "56px"
    },
    "style": {
      "position": "sticky",
      "top": 0,
      "zIndex": 1000,
      "minHeight": "56px",
      "height": "56px"
    },
    "class": {
      "mat-elevation-z1": true
    },
    "actions": [
      {
        "type": "button",
        "buttonType": "mat-icon-button",
        "label": "Refresh",
        "click": "this.event.send({'name': 'refresh'})",
        "icon": "refresh"
      }
    ]
  },
  "content": {
    "layoutStyle": {
      "overflow": "hidden",
      "display": "flex",
      "flex-direction": "column",
      "flex-grow": 1
    },
    "style": {
      "width": "100%",
      "minWidth": "100%",
      "padding": "0",
      "background": "#f1f1f1",
      "display": "flex",
      "flexDirection": "column",
      "flex-grow": 1,
      "height": "100%"
    }
  }
}
