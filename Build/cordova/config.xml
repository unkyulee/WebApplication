        xmlns="http://www.w3.org/ns/widgets"
        xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Andrea Boldrini</name>
    <description>
        CRM for Andrea Boldrini
    </description>
    <author email="unkyulee@gmail.com" href="https://unkyulee.me">
        Unkyu Lee
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
